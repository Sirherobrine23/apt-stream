  "version": "0.2.0",
  "configurations": [
    {
      "name": "Lauch API",
      "type": "node",
      "request": "launch",
      "runtimeExecutable": "ts-node",
      "args": ["src/cli.ts"],
      "env": {
        "DEBUG": "coreutils:*,node_apt:*"
      },
      "cwd": "${workspaceRoot}",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**", "node_modules/**"]
    }

  ]
}